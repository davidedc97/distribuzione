<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='egeosDCL'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User' />
  <inherits name='com.smartgwt.SmartGwt' />
  <!-- We need the JUnit module in the main module,               -->
  <!-- otherwise eclipse complains (Google plugin bug?)           -->
  <inherits name='com.google.gwt.junit.JUnit' />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard' />
  <inherits name='com.googlecode.gwt.crypto.Crypto' />
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" />
  <set-property name="log_DivLogger" value="DISABLED" />
  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='it.gis.egeosDCL.client.egeosDCL' />

  <script src="OpenLayers-2.13.1/OpenLayers.js"></script>
  <script src="OpenLayers-2.13.1/proj4js.js"></script>
  <script src="OpenLayers-2.13.1/defs/EPSG_3003.js"></script>
  <script src="OpenLayers-2.13.1/defs/EPSG_3004.js"></script>
  <script src="OpenLayers-2.13.1/defs/EPSG_40000.js"></script>
  <script src="OpenLayers-2.13.1/defs/EPSG_40001.js"></script>
  <script src="OpenLayers-2.13.1/defs/EPSG_40002.js"></script>
  <script src="printAll.js"></script>
  <script src="jquery-1.6.4.min.js"></script>
  <!-- Specify the paths for translatable code                    -->
  <source path='client' />
  <source path='shared' />

</module>
